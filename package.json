{
  "name": "@kitalive/sfdx-plugin",
  "description": "Kitalive SFDX plugin",
  "version": "0.4.5",
  "author": "Akihiro Ono",
  "bugs": "https://github.com/Kitalive-Inc/sfdx-plugin/issues",
  "dependencies": {
    "@salesforce/command": "^4",
    "@salesforce/core": "^2.23.5",
    "dayjs": "^1.10.4",
    "fast-csv": "^4.3.6",
    "fs-extra": "^9.1.0",
    "iconv-lite": "^0.6.2",
    "jsforce": "^1.10.1",
    "soql-parser-js": "^4.3.0",
    "tslib": "^2",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@oclif/core": "^1",
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^2",
    "@salesforce/ts-sinon": "^1.3.0",
    "@types/jest": "^27.4.1",
    "@types/jsforce": "^1.9.29",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "chai": "^4",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "globby": "^11",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Kitalive-Inc/sfdx-plugin",
  "keywords": [
    "sfdx-plugin",
    "sfdx",
    "plugin",
    "salesforce"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "kit:layout:assignments": {
        "description": "Commands to manage object layout assignments without profiles."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Kitalive-Inc/sfdx-plugin",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "prepare": "husky install",
    "test": "jest --coverage",
    "version": "oclif-dev readme && git add README.md"
  },
  "lint-staged": {
    "src/**/*.ts": "yarn lint --fix",
    "*.ts": "prettier --write"
  }
}
