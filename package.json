{
  "name": "@kitalive/sfdx-plugin",
  "description": "Kitalive SFDX plugin",
  "version": "0.4.6",
  "author": "Akihiro Ono",
  "bugs": "https://github.com/Kitalive-Inc/sfdx-plugin/issues",
  "dependencies": {
    "@salesforce/sf-plugins-core": "^2",
    "dayjs": "^1.11.7",
    "fast-csv": "^4.3.6",
    "fs-extra": "^11.1.1",
    "iconv-lite": "^0.6.3",
    "soql-parser-js": "^4.7.1",
    "tslib": "^2",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@salesforce/cli-plugins-testkit": "^3",
    "@salesforce/dev-config": "^3",
    "@salesforce/dev-scripts": "^3",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1",
    "@swc/core": "^1.3.50",
    "@types/jest": "^29.5.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "chai": "^4",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-salesforce": "^1.2.0",
    "eslint-config-salesforce-license": "^0.2.0",
    "eslint-config-salesforce-typescript": "^1.1.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "^41.1.1",
    "eslint-plugin-sf-plugin": "^1.15.1",
    "husky": "^7.0.4",
    "mocha": "^10",
    "nyc": "^15",
    "oclif": "^3",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "shx": "0.3.4",
    "sinon": "10",
    "ts-node": "^10",
    "typescript": "^4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Kitalive-Inc/sfdx-plugin",
  "keywords": [
    "salesforce",
    "sf-plugin",
    "sfdx-plugin",
    "sf",
    "sfdx",
    "plugin"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sf",
    "topicSeparator": " ",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Kitalive-Inc/sfdx-plugin",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postinstall": "yarn husky install",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "sf-prepack",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "version": "oclif readme"
  }
}
