{
  "name": "@kitalive/sfdx-plugin",
  "description": "Kitalive SFDX plugin",
  "version": "0.2.0",
  "author": "Akihiro Ono",
  "bugs": "https://github.com/Kitalive-Inc/sfdx-plugin/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^3.1.3",
    "@salesforce/core": "^2.23.5",
    "dayjs": "^1.10.4",
    "fast-csv": "^4.3.6",
    "fs-extra": "^9.1.0",
    "iconv-lite": "^0.6.2",
    "jsforce": "^1.10.1",
    "soql-parser-js": "^4.3.0",
    "tslib": "^1",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.4.1",
    "@salesforce/ts-sinon": "^1.3.0",
    "@types/chai": "^4",
    "@types/jsforce": "^1.9.29",
    "@types/mocha": "^5",
    "@types/yargs": "^17.0.8",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Kitalive-Inc/sfdx-plugin",
  "keywords": [
    "sfdx-plugin",
    "sfdx",
    "plugin",
    "salesforce"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "kit:layout:assignments": {
        "description": "Commands to manage object layout assignments without profiles."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Kitalive-Inc/sfdx-plugin",
  "scripts": {
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
