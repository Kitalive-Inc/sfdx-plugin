{
  "name": "@kitalive/sfdx-plugin",
  "description": "Kitalive SFDX plugin",
  "version": "0.4.6",
  "author": "Akihiro Ono",
  "bugs": "https://github.com/Kitalive-Inc/sfdx-plugin/issues",
  "dependencies": {
    "@salesforce/command": "^5",
    "@salesforce/core": "^3",
    "dayjs": "^1.11.7",
    "fast-csv": "^4.3.6",
    "fs-extra": "^11.1.1",
    "iconv-lite": "^0.6.3",
    "soql-parser-js": "^4.7.1",
    "tslib": "^2",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/test": "^2",
    "@salesforce/ts-sinon": "^1",
    "@types/jest": "^29.5.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "globby": "^13",
    "husky": "^7.0.4",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10",
    "typescript": "^4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Kitalive-Inc/sfdx-plugin",
  "keywords": [
    "sfdx-plugin",
    "sfdx",
    "plugin",
    "salesforce"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "kit:layout:assignments": {
        "description": "Commands to manage object layout assignments without profiles."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Kitalive-Inc/sfdx-plugin",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "prepare": "husky install",
    "test": "jest --coverage",
    "version": "oclif-dev readme && git add README.md"
  },
  "lint-staged": {
    "src/**/*.ts": "yarn lint --fix",
    "*.ts": "prettier --write"
  }
}
